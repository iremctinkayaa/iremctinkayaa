package test;
import java.util.Arrays;
import java.util.Random;

public class i2 {

	public static void main(String[] args) {
		Random rnd = new Random ();
		int x = rnd.nextInt (5);
		
		int [] dizi = new int [x] ;
		for  (int i=0; i<dizi.length; i++)
		{
			dizi [i] = rnd.nextInt(5);
		}
		for (int i=0; i<dizi.length; i++)
		{
			System.out.println(dizi [i] + "");
		}

	}

}
